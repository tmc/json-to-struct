# Test cases for struct extraction feature
# Tests the -extract-structs flag to reduce duplication
# flags: -extract-structs

-- simple_duplicate.json --
{
  "user": {
    "id": 1,
    "name": "Alice",
    "active": true
  },
  "admin": {
    "id": 2,
    "name": "Bob",
    "active": false
  }
}

-- simple_duplicate.go --
package test_package

type simple_duplicateStructC801770D struct {
	Active bool    `json:"active,omitempty"`
	ID     float64 `json:"id,omitempty"`
	Name   string  `json:"name,omitempty"`
}

type simple_duplicate struct {
	Admin simple_duplicateStructC801770D `json:"admin,omitempty"`
	User  simple_duplicateStructC801770D `json:"user,omitempty"`
}
